{
  "name": "georgeff/pipeline",
  "description": "Immutable pipeline builder",
  "type": "library",
  "authors": [
    {
      "name": "Mike Georgeff",
      "email": "mike@georgeff.co",
      "role": "Author"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.0",
    "squizlabs/php_codesniffer": "^3.11",
    "friendsofphp/php-cs-fixer": "^3.93"
  },
  "autoload": {
    "psr-4": {
      "Georgeff\\Pipeline\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Georgeff\\Pipeline\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ],
    "analyze": [
      "vendor/bin/phpstan"
    ],
    "phpcs": [
      "vendor/bin/phpcs"
    ]
  }
}
